    "express": "^4.18.2",
    "openai": "^4.19.0",
    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/eslint": "^8",
    "@types/express": "^4",
    "@types/node": "^20.9.0",
    "@types/uuid": "^9",
    "@types/ws": "^8",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.2.2"
  }
}
